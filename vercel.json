{
  "rewrites": [
    {
      "source": "/",
      "destination": "/login"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true
}